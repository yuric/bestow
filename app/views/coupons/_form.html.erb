      <% @coupon.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
<br/>
	<%if user_signed_in? %>
		<%if !current_user.admin?%>
		<div style="display: none;">
			<%= f.label :coupon_owner, t("activerecord.attributes.coupon.coupon_owner", :default => "Coupon owner"), :class => :label %>
			<%= f.text_field :coupon_owner, :readonly => true, :value => current_user.username || "Invalid User" %>
		</div>
			
		<%elsif current_user.admin?%>
		<div style="display: block;">
			<%= f.label :coupon_owner, t("activerecord.attributes.coupon.coupon_owner", :default => "Coupon owner"), :class => :label %>
			<%= f.text_field :coupon_owner, :value => @coupon.coupon_owner || "None" %>
		</div>	
			
		<%else%>
		<%end%>
	<%end%>
	
	<div class="group">
	  <%= f.label :angel_business_name, t("activerecord.attributes.coupon.angel_business_name", :default => "Angel business name"), :class => :label %>
		<%= f.select :angel_business_name, options_for_select(@ba, @coupon.angel_business_name) %> or <%= link_to 'Register a new business', new_businessform_path %> 
		<span class="description">hint: Create/select a business form to have the many of fields bellow automatically filled out for you</span>
	</div>
	
	<div class="group">
	  <%= f.label :qualified_organization_name, t("activerecord.attributes.coupon.qualified_organization_name", :default => "Name of non-profit you are in support of"), :class => :label %>
		<%= f.text_field :qualified_organization_name, :class => 'text_field' %> 
		<span class="description">ex. Make a Wish Foundation</span>
	</div>	

  <div class="group">
    <%= f.label :address_line1, t("activerecord.attributes.coupon.address_line1", :default => "Address line1"), :class => :label %>
    <%= f.text_field :address_line1, :class => 'text_field' %>
    <span class="description">ex: 122 Awesome Street (*No P.O. Boxes Please)</span>
  </div>

  <div class="group">
    <%= f.label :address_line2, t("activerecord.attributes.coupon.address_line2", :default => "Address line2"), :class => :label %>
    <%= f.text_field :address_line2, :class => 'text_field' %>
    <span class="description">ex: Apartment 3327</span>
  </div>

  <div class="group">
    <%= f.label :city, t("activerecord.attributes.coupon.city", :default => "City"), :class => :label %>
    <%= f.text_field :city, :class => 'text_field' %>
    <span class="description">ex: San Franscisco</span>
  </div>

  <div class="group">
    <%= f.label :state, t("activerecord.attributes.coupon.state", :default => "State"), :class => :label %>
	<%= f.select :state, options_for_select(Variables::US_STATES, @coupon.state) %>
  </div>

  <div class="group">
    <%= f.label :country, t("activerecord.attributes.coupon.country", :default => "Country"), :class => :label %>
	<%= f.select :country, options_for_select(Variables::COUTRIES_SUPPORTED, @coupon.country) %>
  </div>

  <div class="group">
    <%= f.label :zipcode, t("activerecord.attributes.coupon.zipcode", :default => "Zipcode"), :class => :label %>
    <%= f.text_field :zipcode, :class => 'text_field' %>
    <span class="description">ex: 12345-6789</span>
  </div>

  <div class="group">
    <%= f.label :angel_business_website, t("activerecord.attributes.coupon.angel_business_website", :default => "Angel business website"), :class => :label %>
    <%= f.text_field :angel_business_website, :class => 'text_field' %>
    <span class="description">ex: www.example.com</span>
  </div>

  <div class="group">
    <%= f.label :support_summary, t("activerecord.attributes.coupon.support_summary", :default => "Support offer summary"), :class => :label %>
    <%= f.text_field :support_summary, :class => 'text_field' %>
    <span class="description">ex: Summarize your support offer in 10 to 160 characters.</span>
  </div>

  <div class="group">
    <%= f.label :angel_business_type, t("activerecord.attributes.coupon.angel_business_type", :default => "Angel business category"), :class => :label %>
	<%= f.select :angel_business_type, options_for_select(Variables::BUSINESS_CATEGORIES, @coupon.angel_business_type) %>
  </div>

  <div class="group">
    <%= f.label :qualified_organization_type, t("activerecord.attributes.coupon.qualified_organization_type", :default => "Non-profit in support of"), :class => :label %>
    <%= f.text_field :qualified_organization_type, :class => 'text_field' %>
    <span class="description">ex: </span>
  </div>

  <div class="group">
    <%= f.label :angel_discription_zoom_zero_qo, t("activerecord.attributes.coupon.angel_discription_zoom_zero_qo", :default => "Intentions Summary"), :class => :label %>
    <%= f.text_field :angel_discription_zoom_zero_qo, :class => 'text_field' %>
    <span class="description">Quick description of why you are supporting the non-profit (12-160 chars)</span>
  </div>

  <div class="group">
    <%= f.label :angel_discription_zoom_zero_ab, t("activerecord.attributes.coupon.angel_discription_zoom_zero_ab", :default => "Observations"), :class => :label %>
    <%= f.text_field :angel_discription_zoom_zero_ab, :class => 'text_field' %>
    <span class="description">Quick observations or overview of your Business</span>
  </div>

  <div class="group">
    <%= f.label :cause_start_date, t("activerecord.attributes.coupon.cause_start_date", :default => "Cause start date"), :class => :label %>
    <%= f.datetime_select :cause_start_date, :class => 'datetime_select' %>
  </div>

  <div class="group">
    <%= f.label :cause_end_date, t("activerecord.attributes.coupon.cause_end_date", :default => "Cause end date"), :class => :label %>
    <%= f.datetime_select :cause_end_date, :class => 'datetime_select' %>
  </div>

  <div class="group">
    <%= f.label :angel_full_discription_zoom_one, t("activerecord.attributes.coupon.angel_full_discription_zoom_one", :default => "More about your business"), :class => :label %>
    <%= f.text_area :angel_full_discription_zoom_one, :class => 'text_area' %>
    <span class="description">ex. This will be shown when user requests to know more about your business</span>
  </div>

  <div class="group">
    <%= f.label :coupon_category, t("activerecord.attributes.coupon.coupon_category", :default => "Coupon category"), :class => :label %>
	<%= f.select :coupon_category, options_for_select(Variables::BUSINESS_CATEGORIES, @coupon.coupon_category) %>
  </div>

  <div class="group">
    <%= f.label :coupon_subcategory, t("activerecord.attributes.coupon.coupon_subcategory", :default => "Coupon subcategory"), :class => :label %>
    <%= f.text_field :coupon_subcategory, :class => 'text_field' %>
    <span class="description">ex: Vegetarian Meals</span>
  </div>

  <div class="group">
    <%= f.label :curr_ip, t("activerecord.attributes.coupon.curr_ip", :default => "Curr ip"), :class => :label %>
    <%= f.text_field :curr_ip, :class => 'text_field' %>
  </div>

  <div class="group">
    <%= f.label :last_ip, t("activerecord.attributes.coupon.last_ip", :default => "Last ip"), :class => :label %>
    <%= f.text_field :last_ip, :class => 'text_field' %>
  </div>

  <div class="group">
    <%= f.label :all_ip, t("activerecord.attributes.coupon.all_ip", :default => "All ip"), :class => :label %>
    <%= f.text_area :all_ip, :class => 'text_area' %>
  </div>
        
<div class="group navform wat-cf">
  <button class="button" type="submit">
    <%= image_tag("web-app-theme/icons/tick.png", :alt => "#{t("web-app-theme.save", :default => "Save")}") %> <%= t("web-app-theme.save", :default => "Save") %>
  </button>
  <span class="text_button_padding"><%= t("web-app-theme.or", :default => "or") %></span>
	<%= link_to t("web-app-theme.cancel", :default => "Cancel"), coupons_path, :class => "text_button_padding link_button" %>
</div>